<h1>Listing invitecodes</h1>

<table>
  <tr>
    <th>Code</th>
    <th>Create user</th>
    <th>Status</th>
    <th>Enable date</th>
    <th>Enable user</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @invitecodes.each do |invitecode| %>
  <tr>
    <td><%= invitecode.code %></td>
    <td><%= invitecode.create_user_id %></td>
    <td><%= invitecode.status %></td>
    <td><%= invitecode.enable_date %></td>
    <td><%= invitecode.enable_user_id %></td>
    <td><%= link_to 'Show', invitecode %></td>
    <td><%= link_to 'Edit', edit_invitecode_path(invitecode) %></td>
    <td><%= link_to 'Destroy', invitecode, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Invitecode', new_invitecode_path %>
